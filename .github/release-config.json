{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## 🔒 Security",
      "labels": ["security"]
    },
    {
      "title": "## 📚 Documentation",
      "labels": ["documentation"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["chore", "dependencies"]
    }
  ],
  "ignore_labels": [
    "ignore",
    "wontfix",
    "duplicate"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "^feat:",
      "target": "feature"
    },
    {
      "pattern": "^fix:",
      "target": "bug"
    },
    {
      "pattern": "^docs:",
      "target": "documentation"
    },
    {
      "pattern": "^chore:",
      "target": "chore"
    }
  ]
}
